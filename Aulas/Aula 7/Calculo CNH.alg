algoritmo "semnome"

var
anoAtual : inteiro
anoNasc: inteiro
idade : inteiro
inicio
escreva("Digite o ano atual: ")
leia(anoAtual)
escreva("Digite o ano de nascimento: ")
leia(anoNasc)
idade <- anoAtual - anoNasc
se (idade >= 18 ) entao
   escreva("Parabéns, voce ja pode tirar cnh")
senao
   escreva("Voce ainda nao pode tirar cnh")
fimSe


fimalgoritmo
